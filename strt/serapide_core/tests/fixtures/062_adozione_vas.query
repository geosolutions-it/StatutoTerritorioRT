{
  "operationName": "AdozioneVas",
  "variables": {
    "codice": "{codice}"
  },
  "query": "query AdozioneVas($codice: String!) {
  modello: procedureAdozioneVas(piano_Codice: $codice) {
    edges {
      node {
        uuid
        risorse(archiviata: false) {
          ...Risorse
        }
      }
    }
  }
}

fragment Risorse on RisorsaNodeConnection {
  edges {
    node {
      ...Risorsa
    }
  }
}

fragment Risorsa on RisorsaNode {
  nome
  uuid
  tipo
  dimensione
  downloadUrl
  lastUpdate
  user {
    fiscalCode
    firstName
    lastName
  }
  label
  tooltip
}
"
}
