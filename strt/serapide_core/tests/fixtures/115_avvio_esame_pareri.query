{
  "operationName": "AvvioEsamePareriSCA",
  "variables": {
    "uuid": "{codice}"
  },
  "query": "mutation AvvioEsamePareriSCA($uuid: String!) {
  avvioEsamePareriSca(uuid: $uuid) {
    vasAggiornata {
      piano {
        ...AzioniPiano
      }
    }
  }
}

fragment AzioniPiano on PianoNode {
  codice
  fase
  azioni {
    ...Azioni
  }
}

fragment Azioni on AzioneNode {
      order
      tipologia
      stato
      qualificaRichiesta
      data
      uuid
      label
      tooltip
      fase
}
"
}
