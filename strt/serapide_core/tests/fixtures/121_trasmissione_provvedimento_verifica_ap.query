{
   "operationName" : "PubblicazioneProvvedimentoVerificaAp",

   "variables" : {
      "uuid" : "{codice}"
   },

   "query" : "mutation PubblicazioneProvvedimentoVerificaAp($uuid: String!) {
  pubblicazioneProvvedimentoVerificaAp(uuid: $uuid) {
    vasAggiornata {
      uuid
      verificaEffettuata
      dataAssoggettamento
      piano {
        ...AzioniPiano
      }
    }
  }
}

fragment AzioniPiano on PianoNode {
  codice
  fase
  azioni {
    ...Azioni
  }
}

fragment Azioni on AzioneNode {
      order
      tipologia
      stato
      qualificaRichiesta
      data
      uuid
      label
      tooltip
      fase
      avvioScadenza
      scadenza
}
"
}