{
   "operationName" : "TrasmissioneOsservazioni",

  "variables" : {
      "codice" : "{codice}"
   },

   "query" : "mutation TrasmissioneOsservazioni($codice: String!) {
  trasmissioneOsservazioni(uuid: $codice) {
    errors
    adozioneAggiornata {
      piano {
        ...AzioniPiano
        __typename
      }
      __typename
    }
    __typename
  }
}

fragment AzioniPiano on PianoNode {
  codice
  fase
  azioni {
    ...Azioni
    __typename
  }
  __typename
}

fragment Azioni on AzioneNode {

      order
      tipologia
      stato
      qualificaRichiesta
      data
      uuid
      label
      tooltip
      fase
      __typename

}"
}
