{
  "operationName": "RevisioneConferenzaPaesaggistica",
  "variables": {
    "codice": "{codice}"
  },
  "query": "mutation RevisioneConferenzaPaesaggistica($codice: String!) {
  revisioneConferenzaPaesaggistica(uuid: $codice) {
    errors
    adozioneAggiornata {
      piano {
        ...AzioniPiano
      }
    }
  }
}

fragment AzioniPiano on PianoNode {
  codice
  fase
  azioni {
    ...Azioni
  }
}

fragment Azioni on AzioneNode {
      order
      tipologia
      stato
      qualificaRichiesta
      data
      uuid
      label
      tooltip
      fase
}
"
}
