{
   "operationName" : "getPiani",

   "variables" : {
      "codice" : "{codice}"
   },

   "query" : "query getPiani($faseCodice: String, $codice: String) {
  piani(fase_Codice: $faseCodice, codice: $codice) {
    edges {
      node {
        ...Piano
      }
    }
  }
}

fragment Piano on PianoNode {
  codice
  tipo: tipologia
  descrizione
  lastUpdate
  dataDelibera
  dataCreazione
  dataAccettazione
  dataAvvio
  dataApprovazione
  alertsCount
  numeroProtocolloGenioCivile
  dataProtocolloGenioCivile
  redazioneNormeTecnicheAttuazioneUrl
  compilazioneRapportoAmbientaleUrl
  conformazionePitPprUrl
  monitoraggioUrbanisticoUrl
  azioni {
    ...Azioni
  }
  ente {
    ipa
    nome
    tipo
  }
  fase
  responsabile {
    ...User
  }
  risorse(archiviata: false) {
    ...Risorse
  }
  soggettoProponente {
        qualifica
        ufficio {uuid nome}
  }
  soggettiOperanti {
      qualificaUfficio {
        qualifica
        ufficio {uuid nome}
      }
  }

  proceduraVas {uuid}
  proceduraAvvio {uuid}
}

fragment Risorse on RisorsaNodeConnection {
  edges {
    node {
      ...Risorsa
    }
  }
}

fragment Risorsa on RisorsaNode {
  nome
  uuid
  tipo
  dimensione
  downloadUrl
  lastUpdate
  user {
    fiscalCode
    firstName
    lastName
  }
  label
  tooltip
}

fragment User on UtenteNode {
  id
  email
  fiscalCode
  firstName
  lastName
  dateJoined
  alertsCount
}

fragment Azioni on AzioneNode {
      order
      tipologia
      stato
      qualificaRichiesta
      eseguibile
      data
      uuid
      label
      tooltip
      fase
      avvioScadenza
      scadenza
}"
}
